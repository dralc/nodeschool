mixin dump(list)
    table
        each val, key in list
            tr
                td= key
                
                if typeof val == 'object' && val.length == undefined
                    td 
                        +dump(val)
                else
                    td= val

            
            
-var data = {a: 1, b: ['b1', 'b2'], c: {d: 3}}
+dump(data)
